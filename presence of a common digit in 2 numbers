 public static boolean hasSharedDigit(int a , int b){
        int f1,f2,s1,s2;
        if(a>99||a<10||b<10||b>99)
            return false;
        else
        {
            f1=a/10;
            f2=a%10;
            s1=b/10;
            s2=b%10;

            if(f1==s1||f1==s2||f2==s1||f2==s2)
                return true;
            else return false;
        }


    }
